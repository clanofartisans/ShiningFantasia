{
  "name": "shining-fantasia",
  "version": "0.0.0",
  "description": "Xi DAT Editor",
  "scripts": {
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "build-dist": "webpack --mode production --env dist-build",
    "watch": "webpack --watch --mode development",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start-dev": "npm run dev && electron ./build/main/main.js",
    "start": "npm run build && electron ./build/main/main.js",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build-dist && electron-builder"
  },
  "author": "Velyn",
  "license": "GPL-3.0-or-later",
  "private": true,
  "main": "./build/main/main.js",
  "build": {
    "appId": "org.somepage.shiningfantasia",
    "files": [
      "build/**/*",
      "package.json"
    ],
    "directories": {},
    "publish": null,
    "win": {
      "target": [
        {
          "target": "portable"
        }
      ],
      "icon": "build/icon.png"
    }
  },
  "devDependencies": {
    "@popperjs/core": "^2.11.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "bootstrap": "^5.1.3",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.1",
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5",
    "eslint": "^8.5.0",
    "html-webpack-plugin": "^5.5.0",
    "iconv-lite": "^0.6.3",
    "mini-css-extract-plugin": "^2.4.5",
    "purgecss-webpack-plugin": "^4.1.3",
    "sass": "^1.45.0",
    "sass-loader": "^12.4.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "vue": "^3.2.26",
    "vue-loader": "^17.0.0",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "vuex": "^4.0.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-merge": "^5.8.0"
  }
}
